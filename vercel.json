{
    "buildCommand": "npm run build",
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Credentials", "value": "true" },
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,POST" },
          { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
        ]
      }
    ],
    "redirects": [
      { 
        "source": "/brand", 
        "destination": "https://storage.googleapis.com/blz-one-9e383.appspot.com/PDF/BlazedBrandBook/1-3.pdf", 
        "permanent": false 
      },
      { 
        "source": "/humans", 
        "destination": "https://storage.googleapis.com/blz-one-9e383.appspot.com/TXT/humans.txt", 
        "permanent": true 
      },
      {
        "source": "/license",
        "destination": "https://storage.googleapis.com/blz-one-9e383.appspot.com/TXT/LICENSE.txt",
        "permanent": true 
      },
      {
        "source": "/flyer",
        "destination": "https://storage.googleapis.com/blz-one-9e383.appspot.com/PDF/Blazed-One-Flyer.pdf",
        "permanent": true
      }
    ]
  }
  